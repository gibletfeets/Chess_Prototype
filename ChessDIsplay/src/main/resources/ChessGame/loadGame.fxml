<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="150.0" prefWidth="310.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ChessGame.LoadGameController">
   <children>
      <ComboBox fx:id="comboBox" layoutX="14.0" layoutY="114.0" prefHeight="25.0" prefWidth="215.0" />
      <Button fx:id="loadButton" layoutX="248.0" layoutY="114.0" mnemonicParsing="false" onAction="#loadGame" text="Load" />
      <ImageView fitHeight="100.0" fitWidth="150.0" layoutX="72.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@PNG/wB.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
